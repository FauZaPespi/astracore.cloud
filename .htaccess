RewriteEngine On
RewriteBase /astracore.cloud/

# Allow static files (.js, .css, .png, .svg, etc.)
RewriteCond %{REQUEST_URI} \.(js|css|png|jpg|jpeg|gif|svg|ico|webp|woff|woff2|ttf|eot)$ [NC]
RewriteRule ^ - [L]

# Donâ€™t rewrite existing files or directories
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Send everything else to index.php
RewriteRule ^ index.php [QSA,L]
